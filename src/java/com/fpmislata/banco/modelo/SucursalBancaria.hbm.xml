<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.fpmislata.banco.modelo.SucursalBancaria" table="cuentabancaria" >
        <id column="idSucursalBancaria" name="idSucursalBancaria" type="integer">
            <generator class="identity" />
        </id>
        <property name="entidadBancaria" column="entidadBancaria"  />
        <property name="codigoSucursal" column="codigoSucursal" />
        <property name="nombreSucursal" column="nombreSucursal" />
    </class>
</hibernate-mapping>